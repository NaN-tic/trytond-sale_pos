<tryton>
    <data>
        <record model="ir.ui.view" id="pos_device_form">
            <field name="model">sale_pos.device</field>
            <field name="type">form</field>
            <field name="name">pos_device_form</field>
        </record>
        <record model="ir.ui.view" id="pos_device_tree">
            <field name="model">sale_pos.device</field>
            <field name="type">tree</field>
            <field name="name">pos_device_tree</field>
        </record>

        <record model="ir.action.act_window" id="act_pos_device">
            <field name="name">POS Device</field>
            <field name="res_model">sale_pos.device</field>
        </record>

        <menuitem name="POS Devices" parent="sale.menu_configuration"
            id="menu_pos_device" icon="tryton-list" action="act_pos_device"/>
    </data>
</tryton>
